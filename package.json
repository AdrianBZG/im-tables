{
  "name": "intermine-tables",
  "version": "2.0.0",
  "description": "ui widgets for displaying query results in interactive tables",
  "config": {
    "port": "9000"
  },
  "scripts": {
    "prestart": "npm run watch-less & watch 'npm run build-test-indices' src/ --wait=20 &",
    "start": "./bin/dev-server",
    "pretest": "npm run pre-compile && npm run compile",
    "test": "mocha --compilers coffee:coffee-script/register test/unit/*",
    "watch-src": "watch 'npm run build' src/ --wait=20",
    "watch-less": "watch 'npm run less' less/ --wait=20",
    "watch": "npm run watch-src & npm run watch-js & npm run watch-less",
    "pre-compile": "./bin/clean",
    "compile-coffee": "coffee --compile --output build/ src/",
    "cp-templates": "mkdir -p build/templates && cp templates/* build/templates",
    "inline-templates": "./bin/inline-templates",
    "inject-version": "./bin/inject-version.js",
    "do-compile": "npm run compile-coffee && npm run inject-version && npm run cp-templates && npm run inline-templates",
    "compile": "npm run pre-compile && npm run do-compile",
    "bundle-test-indices": "./bin/bundle-test-indices",
    "build-test-indices": "npm run compile && npm run less && npm run bundle-test-indices",
    "bundle": "browserify build/main.js > dist/main.js",
    "less": "lessc --include-path=less:node_modules less/main.less > dist/main.css",
    "build": "npm run compile && npm run bundle && npm run less"
  },
  "keywords": [
    "javascript",
    "webservice",
    "InterMine",
    "Bio-Informatics"
  ],
  "main": "./js/imtables.js",
  "browser": {
    "d3js": "./bower_components/d3/d3.min.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "d3js": {
      "exports": "d3",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "author": {
    "name": "Alex Kalderimis",
    "email": "alex@intermine.org",
    "twitter": "alexkalderimis"
  },
  "homepage": "http://www.intermine.org",
  "license": "LGPL",
  "repository": {
    "type": "git",
    "url": "git://github.com/alexkalderimis/im-tables.git"
  },
  "contributors": [],
  "engines": {
    "node": "~0.10.0"
  },
  "directories": {
    "tests": "test"
  },
  "devDependencies": {
    "backbone": "^1.1.2",
    "bootstrap": "^3.3.1",
    "bower": "latest",
    "brfs": "^1.2.0",
    "browserify": "^7.0.0",
    "browserify-shim": "~3.8.1",
    "codo": "~0.3.0",
    "coffee-script": "~1.8.0",
    "coffeelint": ">= 0.5.4",
    "connect": "~2.7.3",
    "es6-promise": "^2.0.1",
    "font-awesome": "^4.2.0",
    "imjs": "^3.7.2",
    "jquery": "^2.1.1",
    "jquery-ui": "^1.10.5",
    "less": "^2.1.1",
    "mocha": "^2.0.1",
    "prefix-css-node": "~0.4.0",
    "q": "~0.9.0",
    "selenium-webdriver": "^2.44.0",
    "should": "^4.4.1",
    "stringify": "^3.0.0",
    "typeahead.js": "^0.10.5",
    "uglify-js": "~2.2.5",
    "watch": "^0.13.0"
  },
  "dependencies": {
    "brfs-htmlmin": "^1.0.0",
    "serve": "^1.4.0",
    "underscore": "^1.7.0"
  }
}
