<tr>
  <td class="im-field-name"><%- _.last(parts) %></td>
  <td class="im-field-value <%- field.toLowerCase() %> <%- type %>">
    <%- value %>
    <% if (tooLong) { %>
      <span class="im-overspill"><%- valueOverspill %></span>
      <a class="im-too-long">
        <span class="im-ellipsis">...</span>
        <%= Icons.icon('More') %>
      </a>
    <% } %>
  </td>
</tr>
