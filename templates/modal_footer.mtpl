<% if (error) { %>
    <div class="pull-left alert alert-danger">
        <%= Icons.icon('Error') %>
        <strong><%- Messages.get('Error') %></strong>
        <%- error.message || error %>
    </div>
<% } %>

<button type="button" class="btn btn-cancel">
    <%- dismissAction %>
</button>
<button type="button"
        <%= (error) ? 'disabled' : void 0 %>
        class="btn btn-primary">
    <%- primaryAction %>
</button>

