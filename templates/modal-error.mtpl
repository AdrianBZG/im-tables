<% if (error) { %>
    <div class="pull-left alert alert-danger">
        <% if (!error.cannotDismiss) { %>
            <button type="button" class="dismiss">
                <span aria-hidden="true">&times;</span>
                <span class="sr-only">Close</span>
            </button>
        <% } %>
        <%= Icons.icon('Error') %>
        <strong><%- Messages.get('Error') %></strong>
        <%- error.message || error %>
    </div>
<% } %>
