<% if (model.error) { %>
    <% /* Oh noes - something went wrong. */ %>
    <div class="alert alert-warning">
        <%= Icons.icon('Error') %>
        <strong><%- Messages.getText('Error') %></strong>
        <% if (data.error.key) { %>
            <%- Messages.getText(data.error.key) %>
        <% } else { %>
            <%- data.error.message || data.error %>
        <% } %>
    </div>
<% } else if (model.initialized) { %>
    <% /* Huzzah - there is data, which will be added here as child views */ %>
    <div class="im-summary-vis"></div>
    <div class="im-summary-items"></div>
<% } else { %>
    <% /* Boo - no data. Show an indeterminate progress bar while we wait. */ %>
    <div class="progress progress-info progress-striped active">
        <div class="bar" style="width:100%"></div>
    </div>
<% } %>
