<div class="im-position-controls">
    <% if (index > 0) { %>
        <%= Icons.iconWithProps('ASC', {
            className: 'im-move-up',
            title: Messages.getText('columns.MoveUp')
        }) %>
    <% } %>
    <% if (!isLast) { %>
        <%= Icons.iconWithProps('DESC', {
            className: 'im-move-down',
            title: Messages.getText('columns.MoveDown')
        }) %>
    <% } %>
</div>

<span class="im-path-name">
    <% _.each(parts, function (part) { %>
        <span class="im-name-part"><%- part %></span>
    <% }); %>
</span>

<span class="pull-right im-remove-view" title="<%- Messages.getText('columns.RemoveColumn') %>">
    <%= Icons.icon('Remove') %>
</span>

